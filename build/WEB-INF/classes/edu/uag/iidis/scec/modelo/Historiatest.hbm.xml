<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  

<hibernate-mapping>
    <class name="edu.uag.iidis.scec.modelo.Historiatest"
           table="Historiatest"
           dynamic-update="false"
           dynamic-insert="false">

        <id name="idh"
            column="idh"
            type="java.lang.Long"
            unsaved-value="null" >
        </id>

        <property name="idr"
            type="java.lang.Long"
            not-null="true" />

        <property name="idrc"
            type="java.lang.Long"
            not-null="true" />

        <property name="idt"
            type="java.lang.Long"
            not-null="true" />

        <property name="idp"
            type="java.lang.Long"
            not-null="true" />

        <property name="respuesta"
            type="java.lang.String"
            not-null="true" />

        <property name="pregunta"
            type="java.lang.String"
            not-null="true" />

        <property name="nombre"
            type="java.lang.String"
            not-null="true" />
            
    </class>

</hibernate-mapping>
